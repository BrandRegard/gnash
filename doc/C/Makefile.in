gnashref.html gnashref.pdf gnash_ref.info: gnashref.xml $(REFMAN) $(COMMON)

# Convienience rules for developers of these manuals. By default
# all output formats are built if the Docbook tools are installed
# and working correctly.
html: $(HTMLS)
pdf:  $(PDFS)
man:  $(MANPAGES) $(EXTRAMANPAGES)
info: $(INFOS)
@DOCBOOK_TRUE@texi: $(TEXIS)

@ENABLE_FOP_TRUE@.xml.pdf:
@ENABLE_FOP_TRUE@	-$(FOP) -xsl $(docbook_styles)/fo/docbook.xsl -xml $< -pdf $@
@ENABLE_FOP_FALSE@@ENABLE_XMLTEX_TRUE@.xml.fo:
@ENABLE_FOP_FALSE@@ENABLE_XMLTEX_TRUE@	$(XSLTPROC) --output $@ --nonet $(docbook_styles)/fo/docbook.xsl $<

@ENABLE_FOP_FALSE@@ENABLE_XMLTEX_TRUE@.fo.pdf:
@ENABLE_FOP_FALSE@@ENABLE_XMLTEX_TRUE@	$(PDFXMLTEX) -interaction=nonstopmode $<
@ENABLE_FOP_FALSE@@ENABLE_XMLTEX_FALSE@.xml.pdf:
@ENABLE_FOP_FALSE@@ENABLE_XMLTEX_FALSE@	touch $@

.xml.html:
@ENABLE_HTML_TRUE@	-$(XSLTPROC) --output $@ --nonet $(docbook_styles)/html/docbook.xsl $<
@ENABLE_HTML_TRUE@	@$(RM) $*.txml
@ENABLE_HTML_FALSE@	@if test -d $(srcdir)/preformatted; then \
@ENABLE_HTML_FALSE@	  echo "WARNING: Linking to preformatted version of $@, it could be out of date."; \
@ENABLE_HTML_FALSE@	  $(LN_S) -f $(srcdir)/preformatted/$@.in ./$@; \
@ENABLE_HTML_FALSE@	else \
@ENABLE_HTML_FALSE@	  touch $@; \
@ENABLE_HTML_FALSE@	fi

# The first test seems to be the default for Fedora/Redhat/Centos based distributions,
# which use a wrapper script for jade.
# Ubuntu/Debian uses a real perl script that actually does the real work
.xml.texi:
@ENABLE_TEXI_TRUE@	-if test x$(DB2X_TEXI) != x; then \
@ENABLE_TEXI_TRUE@	  out=`echo $* | sed -e 's:gnash:gnash_:'`; \
@ENABLE_TEXI_TRUE@	  $(DB2X_TEXI) --encoding=us-ascii//TRANSLIT --string-param directory-description="Gnash" --string-param output-file=$${out} $<; \
@ENABLE_TEXI_TRUE@	  $(MAKEINFO) --force $${out}.texi; \
@ENABLE_TEXI_TRUE@	else \
@ENABLE_TEXI_TRUE@	  basefile="$*"; \
@ENABLE_TEXI_TRUE@          $(DB2X_XSLTPROC) -s texi $< --output $${basefile}.txml; \
@ENABLE_TEXI_TRUE@	  $(DB2X_TEXIXML) --info --encoding=us-ascii//TRANSLIT $${basefile}.txml ; \
@ENABLE_TEXI_TRUE@	  rm $${basefile}.txml; \
@ENABLE_TEXI_TRUE@	fi

.texi.info:
@ENABLE_TEXI_TRUE@	-$(MAKEINFO) --force $<
@ENABLE_TEXI_FALSE@	touch $@;

.man-xml.1:
@ENABLE_MAN_TRUE@	-if test x$(DB2X_MAN) != x; then \
@ENABLE_MAN_TRUE@	  $(DB2X_MAN) $?; \
@ENABLE_MAN_TRUE@        else \
@ENABLE_MAN_TRUE@	  $(DB2X_XSLTPROC) -s man $? -o $*.mxml; \
@ENABLE_MAN_TRUE@	  $(DB2X_MANXML) $*.mxml; \
@ENABLE_MAN_TRUE@	  $(RM) $*.mxml; \
@ENABLE_MAN_TRUE@	fi
@ENABLE_MAN_FALSE@	@if test -d $(srcdir)/preformatted; then \
@ENABLE_MAN_FALSE@	  echo "WARNING: Linking to preformatted version of $@, it could be out of date."; \
@ENABLE_MAN_FALSE@	  $(LN_S) -f $(srcdir)/preformatted/$@.in ./$@; \
@ENABLE_MAN_FALSE@	else \
@ENABLE_MAN_FALSE@	  touch $@; \
@ENABLE_MAN_FALSE@	fi

# As these man page alias files are very small, we just copy them
