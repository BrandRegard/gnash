      if test -f ${with_npapi_incl}/npapi.h; then
        ac_cv_path_npapi_incl="`(cd ${with_npapi_incl}; pwd)`"
      else
      	AC_MSG_ERROR([${with_npapi_incl} directory doesn't contain npapi.h])
      fi
    fi
  ])


