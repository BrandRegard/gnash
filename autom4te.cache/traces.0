m4trace:macros/swfmill.m4:28: -1- AC_DEFUN([AC_PATH_SWFMILL], [
  SWFMILL=""

  AC_ARG_WITH(swfmill, AC_HELP_STRING([--with-swfmill=<path>], [Path to the swfmill executable]), [
    case "${withval}" in
